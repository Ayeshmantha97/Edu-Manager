<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="context" prefHeight="185.0" prefWidth="243.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.developersstack.edumanagment.controller.RestPasswordFormController">
   <children>
      <Label layoutX="24.0" layoutY="22.0" text="Reset Password">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button layoutX="24.0" layoutY="122.0" mnemonicParsing="false" onAction="#changePasswordOnAction" prefHeight="25.0" prefWidth="200.0" text="Change">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Separator layoutX="23.0" layoutY="57.0" prefWidth="200.0" />
      <PasswordField fx:id="txtNewPassword" layoutX="23.0" layoutY="78.0" prefHeight="25.0" prefWidth="200.0" promptText="New Password">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
